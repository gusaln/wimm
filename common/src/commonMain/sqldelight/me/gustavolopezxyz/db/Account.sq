selectAll:
SELECT * FROM account;

selectById:
SELECT * FROM account WHERE id IN ?;

insertAccount:
INSERT INTO account(name, balance_currency, balance_value, initial_value)
VALUES (:name, :balance_currency, :initial_value, :initial_value);


updateAccount:
UPDATE account
SET
    name = :name,
    balance_currency = :balance_currency,
    balance_value = :balance_value,
    initial_value = :initial_value
WHERE id = :id;